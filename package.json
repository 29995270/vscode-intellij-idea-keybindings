{
    "name": "intellij-idea-keybindings",
    "version": "0.3.1",
    "publisher": "k--kato",
    "engines": {
        "vscode": "^1.12.0"
    },
    "displayName": "IntelliJ IDEA Keybindings",
    "description": "Port of IntelliJ IDEA Keybindings, including for WebStorm, PyCharm, PHP Storm, etc.",
    "categories": [
        "Keymaps"
    ],
    "galleryBanner": {
        "color": "#4F87FF",
        "theme": "light"
    },
    "keywords": [
        "IntelliJ",
        "Keybinding"
    ],
    "main": "./out/extension",
    "activationEvents": [
        "*"
    ],
    "dependencies": {},
    "devDependencies": {
        "vscode": "^1.1.0",
        "gulp": "~3.9.1",
        "gulp-cli": "~1.3.0",
        "gulp-strip-json-comments": "~2.1.0",
        "gulp-rename": "~1.2.2",
        "gulp-jsbeautifier": "~2.1.0",
        "typescript": "^2.3.3",
        "tslint": "^5.1.0",
        "@types/node": "^7.0.22",
        "npm-run-all": "^4.0.2"
    },
    "extensionDependencies": [],
    "isAMD": false,
    "scripts": {
        "gulp": "./node_modules/.bin/gulp build",
        "compile": "tsc -watch -p ./",
        "build": "npm-run-all gulp compile",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "vscode:prepublish": "tsc -p ./"
    },
    "icon": "index.svg",
    "license": "MIT",
    "homepage": "https://github.com/k--kato/vscode-intellij-idea-keybindings#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/k--kato/vscode-intellij-idea-keybindings"
    },
    "bugs": {
        "url": "https://github.com/k--kato/vscode-intellij-idea-keybindings/issues"
    },
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "VS Code IntelliJ IDEA configuration",
            "properties": {
                "intellij.mac": {
                    "type": "boolean",
                    "default": false,
                    "description": "macOS keybindings."
                },
                "intellij.win": {
                    "type": "boolean",
                    "default": false,
                    "description": "Windows keybindings."
                },
                "intellij.linux": {
                    "type": "boolean",
                    "default": false,
                    "description": "Linux keybindings."
                }
            }
        },
        "keybindings": [



            {
                "key": "ctrl+y",
                "mac": "cmd+backspace",
                "command": "intellij.resolveKeybinding",
                "args": "editor.action.deleteLines",
                "when": "editorTextFocus && !editorReadonly",
                "intellij": "Delete line at caret"
            }
        ]
    }
}